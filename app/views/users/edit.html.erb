<div class="col-md-10 col-md-offset-1">
  <h1>User info</h1>
  <%= form_for(@user) do |f| %>
    <h4>Name</h4>
    <%= f.text_field :name%>
    <h4>Body</h4>
    <%= f.text_area :introduction%> 
    <h4>Image</h4>
    <%= f.attachment_field :profile_image%>
    <%= f.submit 'Update User'%> 
  <% end %>
</div>
