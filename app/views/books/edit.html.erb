<div class="col-md-6 col-md-offset-3">
  <h1>Editing Book</h1>
  <%= form_for(@book) do |f| %>
    <h4>Title</h4>
    <%= f.text_field :title%>
    <h4>Body</h4>
    <%= f.text_area :body%> 
    <%= f.submit 'Update Book'%> 
  <% end %>
  <%=link_to"Show",book_path(@book)%>
  <span>|</span>
  <%=link_to"Back",books_path %>
</div>
